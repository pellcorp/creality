# https://www.printables.com/model/1170906-klicky-pcb-probe-for-creality-k1

-- klicky.cfg
[probe]
x_offset: -34.0
y_offset: 0

[bed_mesh]
mesh_min: -7,0
mesh_max: 268,287

[screws_tilt_adjust]
__action__: DELETED

[axis_twist_compensation]
__action__: DELETED

-- printer.cfg
[stepper_y]
position_max: 226

-- klicky_macro.cfg
[gcode_macro _KLICKY_VARIABLES]
variable_dock_x_pos: -8
variable_dock_y_pos: 300
variable_pre_dock_y_pos: 250
variable_post_dock_x_pos: 18
